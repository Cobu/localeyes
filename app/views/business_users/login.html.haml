#wrapper.business_users_login
  #content.business_users_login
    %h1.inset Sign in
    .alert
      = alert
    = form_for(@user, :url=> login_submit_business_users_path) do |f|
      .field
        = f.label :email
        = f.text_field :email
      .field
        = f.label :password
        = f.password_field :password

      = f.submit "Sign in", :id=>"log_in", :class=>"button"
